+        fs.writeFileSync(filePath, content);
+    }
+}
+
+// Main execution
+const htmlFiles = findHtmlFiles('.');
+console.log(`Found ${htmlFiles.length} HTML files`);
+
+htmlFiles.forEach(file => {
+    fixSecurityIssues(file);
+});
+
