{
  "name": "yang9112.github.io",
  "version": "1.0.0",
  "description": "Personal blog website - Modernized version",
  "main": "index.html",
  "scripts": {
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/css/style.min.css css/style.css",
    "minify-js": "uglifyjs js/gallery.js js/totop.js -o dist/js/bundle.min.js",
    "dev": "serve -s . -p 4000",
    "security-check": "npm audit",
    "update-deps": "npm-check-updates -u"
  },
  "keywords": [
    "blog",
    "personal",
    "github-pages",
    "static-site"
  ],
  "author": "Anysubway",
  "license": "MIT",
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^12.0.0",
    "npm-check-updates": "^16.14.20",
    "serve": "^14.2.1",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "jquery": "^3.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
